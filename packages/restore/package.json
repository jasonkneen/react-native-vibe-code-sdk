    "lucide-react": "^0.396.0",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "next": ">=15.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@react-native-vibe-code/tsconfig": "workspace:*",
    "@types/react": "^19.0.1",
    "tsup": "^8.5.1",
    "typescript": "^5.5.4"
  }
}
