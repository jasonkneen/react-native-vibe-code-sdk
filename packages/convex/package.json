{
  "name": "@react-native-vibe-code/convex",
  "version": "0.0.1",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@react-native-vibe-code/database": "workspace:*",
    "@react-native-vibe-code/pusher": "workspace:*",
    "@react-native-vibe-code/error-manager": "workspace:*",
    "@e2b/code-interpreter": "2.1.0"
  },
  "devDependencies": {
    "@react-native-vibe-code/tsconfig": "workspace:*",
    "tsup": "^8.5.1",
    "typescript": "^5.5.4"
  }
}
