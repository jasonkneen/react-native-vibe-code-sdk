  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@react-native-vibe-code/config": "workspace:*",
    "@react-native-vibe-code/database": "workspace:*",
    "@polar-sh/better-auth": "^1.0.8",
    "@polar-sh/sdk": "^0.34.8",
    "better-auth": "^1.2.10"
  },
  "peerDependencies": {
    "next": ">=14",
    "react": ">=18"
  },
  "devDependencies": {
    "@react-native-vibe-code/tsconfig": "workspace:*",
    "@types/react": "19.2.7",
    "tsup": "^8.5.1",
    "typescript": "^5.5.4"
  }
}
