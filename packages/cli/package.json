{
  "name": "rnvibecode",
  "version": "0.0.1",
  "license": "MIT",
  "description": "CLI to scaffold React Native Vibe Code projects with Claude Code integration",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "rnvibecode": "./dist/cli.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "copy-template": "node scripts/copy-template.js",
    "prepublishOnly": "pnpm copy-template && pnpm build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "ora": "^8.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@react-native-vibe-code/tsconfig": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.2.0",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.5.1",
    "typescript": "^5.5.4"
  }
}
