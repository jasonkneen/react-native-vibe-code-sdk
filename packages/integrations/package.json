    "@tiptap/suggestion": "^2.12.0",
    "ai": "^3.4.0",
    "@ai-sdk/anthropic": "^1.0.0",
    "exa-js": "^1.0.0",
    "lucide-react": "^0.396.0",
    "tippy.js": "^6.3.7",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@react-native-vibe-code/tsconfig": "workspace:*",
