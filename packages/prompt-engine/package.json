{
  "name": "@react-native-vibe-code/prompt-engine",
  "version": "0.0.1",
  "license": "MIT",
  "description": "AI prompt templates and guidelines for React Native Vibe Code",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./sections": {
      "types": "./dist/prompts/sections/index.d.ts",
      "import": "./dist/prompts/sections/index.mjs",
      "require": "./dist/prompts/sections/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@react-native-vibe-code/tsconfig": "workspace:*",
    "tsup": "^8.5.1",
    "typescript": "^5.5.4"
  }
}
